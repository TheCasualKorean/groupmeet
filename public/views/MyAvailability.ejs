<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/assets/styles.css">
    <link rel="icon" href="/assets/favicon.png">
    <link rel="stylesheet" href="/assets/styles.css"/>
    <link rel="stylesheet" href="../assets/styles.css" type="text/css">
    <link rel="stylesheet" href="../assets/styles.css" type="text/css"/>

    <title>My Availability</title>
</head>
<body class="peachBody">
  <%- include('./partials/_TopNavigationPartial.html') %>
    <div class="grid_12" id="availabilityGrid">
      <header id="myAvailability">
          <h1 id="availabilityHeader">My Availability</h1>
          <a href="/editAvailability" type="submit" value="Submit" class="groupmeet-btn accountBtns">Edit</a>
      </header>
        <div class="meetingTimes">
          <table>
              <tr>
                  <td class="weekCalendar" id="startTime">Start</td>
                  <td class="weekCalendar" id="endTime">End</td>
                  <td class="weekCalendar">Mon</td>
                  <td class="weekCalendar">Tues</td>
                  <td class="weekCalendar">Wed</td>
                  <td class="weekCalendar">Thurs</td>
                  <td class="weekCalendar">Fri</td>
                  <td class="weekCalendar">Sat</td>
                  <td class="weekCalendar">Sun</td>
              </tr>
              <tr>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <p>11:00am</p>
                    </div></th>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <p>3:30pm</p>
                    </div></th>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <img src="/assets/check.png" id="checkImg">
                    </div></th>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <img src="/assets/check.png" id="checkImg">
                    </div></th>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <img src="/assets/x.png" id="xImg">
                    </div></th>
                  <th class="timeRow">
                      <div class="availabilityListItem">
                        <img src="/assets/check.png" id="checkImg">
                      </div></th>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <img src="/assets/x.png" id="xImg">
                    </div></th>
                  <th class="timeRow">
                    <div class="availabilityListItem">
                      <img src="/assets/x.png" id="xImg">
                    </div></th>
                  <th class="timeRow">
                      <div class="availabilityListItem">
                        <img src="/assets/check.png" id="checkImg">
                      </div></th>
              </tr>
          </table>
        </div>
    </div>
</body>
