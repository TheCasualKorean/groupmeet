<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/assets/styles.css">
    <link rel="icon" href="/assets/favicon.png">
    <link rel="stylesheet" href="/assets/styles.css"/>
    <link rel="stylesheet" href="../assets/styles.css" type="text/css">
    <link rel="stylesheet" href="../assets/styles.css" type="text/css"/>

    <title>Dashboard</title>
</head>
<body>
    <%- include('./partials/_TopNavigationPartial.html') %>

    <div class="grid_12">
         <header id="dashboard">
                <h1 id="dashboardHeader">This Week's Meeting Time - </h1>
         </header>
        <div id="meetingTimes">
                <table>
                    <tr>
                        <td class="weekCalendar">Mon</td>
                        <td class="weekCalendar">Tues</td>
                        <td class="weekCalendar">Wed</td>
                        <td class="weekCalendar">Thurs</td>
                        <td class="weekCalendar">Fri</td>
                        <td class="weekCalendar">Sat</td>
                        <td class="weekCalendar">Sun</td>
                    </tr>
                </table>
            </div>
            <div class="groupLinks">
              <div id="addURL">
                <input size="100" placeholder="URL: https://app.groupme.com/chats" type="text" id="url" name="url">
                <button type="submit" value="Submit" class="groupmeet-btn urlBtn">Add URL</button>
              </div>
              <div id="addComment">
                <input size="100" placeholder="COMMENT: when is our assignment due?" type="text" id="comment" name="comment">
                <button type="submit" value="Submit" class="groupmeet-btn commentBtn">Add Comment</button>
              </div>
            </div>
            <div class="grid_6" id="urlBox">
                <h2 id="boxHeader">Urls</h2>
            </div>
            <div class="grid_6" id="commentBox">
                <h2 id="boxHeader">Comments</h2>

        </div>
